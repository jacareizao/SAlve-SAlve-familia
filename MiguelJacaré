/*
public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        int quantidadeMaiores50 = 0;
        double somaAlturas10a20 = 0;
        int contagemAlturas10a20 = 0;
        int quantidadeInferior40 = 0;
        int totalPessoas = 10;

     
